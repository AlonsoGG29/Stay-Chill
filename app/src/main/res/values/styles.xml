<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="boton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/red</item>
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">@font/lora_semibolditalic</item>
    </style>



    <style name="buscadorEstilo" parent="Widget.AppCompat.SearchView">
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="queryBackground">@android:color/transparent</item>
        <item name="android:hint">@string/buscar</item>
    </style>


</resources>