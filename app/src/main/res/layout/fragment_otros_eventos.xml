<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fr_otros"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <TextView
        android:id="@+id/noEventos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/no_eventos_agregado"
        android:textColor="@color/black"
        android:fontFamily="@font/lora_semibold"
        app:layout_constraintEnd_toEndOf="@+id/fr_otros"
        app:layout_constraintStart_toStartOf="@+id/fr_otros"
        app:layout_constraintBottom_toBottomOf="@id/fr_otros" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="650dp"
        app:layout_constraintEnd_toEndOf="@id/fr_otros"
        app:layout_constraintStart_toStartOf="@id/fr_otros"
        app:layout_constraintTop_toBottomOf="@id/fr_otros" />

</androidx.constraintlayout.widget.ConstraintLayout>
